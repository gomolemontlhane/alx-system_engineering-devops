#!/usr/bin/env bash
# This script displays the content of the current directory in a list format, showing only the part after the first dash

for file in *; do
    if [ -f "$file" ] && [ "$file" != "$0" ]; then
        # Extract the part after the first dash
        name=$(echo "$file" | cut -d'-' -f2-)
        echo "$name"
    fi
done
